<view class="index">
  <view class="page">
    <view class="index-profile">
      <view class="index-profile__img" style="background-image: url('{{profile.avatarUrl}}')"></view>
    </view>
    <view class="index-title">
      {{ title }}
    </view>
  </view>


  <view class="page__bd">
    <view class="weui-cells weui-cells_after-title">
      <navigator url="/pages/gallery/gallery" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__bd">印象苏研</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>

      <navigator url="/pages/post-my/post-my" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__bd">我的帖子</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
      <navigator url="/pages/feedback/feedback" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__bd">意见反馈</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
      <navigator url="/pages/about/about" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__bd">关于我们</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
    </view>
  </view>
</view>